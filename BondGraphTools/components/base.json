{
  "name": "Basic Components",
  "id":"base",
  "components": {
    "0": {
      "name": "Equipotential Node",
      "ports": {
        "i": null
      },
      "constitutive_relations": [
        "sum(f_i)",
        "e_i - e_0"
      ]
    },
    "1": {
      "name": "Equal Flow Node",
      "ports": {
        "i":null
      },
      "constitutive_relations": [
        "sum(e_i)",
        "f_i - f_0"
      ]
    },
    "R": {
      "name": "Generalised Linear Resistor",
      "ports": {
        "0": null
      },
      "params": {
        "r": {
          "name": "Resistance",
          "value": 1000,
          "range": [
            0,
            "inf"
          ]
        }
      },
      "constitutive_relations": [
        "e_0 - f_0*r"
      ]
    },
    "C": {
      "name": "Generalised Linear Capacitor",
      "ports": {
        "0": null
      },
      "params": {
        "c": {
          "name": "Capacitance",
          "value": 1000,
          "range": [
            0,
            "inf"
          ]
        }
      },
      "state_vars":{
        "q_0":"Generalised Position"
      },
      "constitutive_relations": [
        "q_0 - c*e_0"
      ]
    },
    "I": {
      "name": "Generalised Linear Inductor",
      "ports": {
        "0": null
      },
      "state_vars":{
        "p_0":"Generalised Momentum"
      },
      "params": {
        "L": {
          "name": "Inductance",
          "value": 2000,
          "range": [
            0,
            "inf"
          ]
        }
      },
      "constitutive_relations": [
        "p_0 - L*f_0"
      ]
    },
    "Se": {
      "name": "Effort Source",
      "ports": {
        "0": null
      },
      "params": {
        "e": null
      },
      "constitutive_relations": [
        "e_0 + e"
      ]
    },
    "Sf": {
      "name": "Flow Source",
      "ports": {
        "0": null
      },
      "params": {
        "f": null
      },
      "constitutive_relations": [
        "f_0 + f"
      ]
    },
    "TF": {
      "name": "Linear Transformer",
      "ports": {
        "0": {
          "name": "Primary"
        },
        "1": {
          "name": "Secondary"
        }
      },
      "params": {
        "a": {
          "name": "Ratio",
          "value": 1
        }
      },
      "constitutive_relations": [
        "e_1 - a*e_0",
        "f_0 - a*f_1"
      ]
    },
    "GY": {
      "name": "Linear Gyrator",
      "ports": {
        "0": {
          "name": "Primary"
        },
        "1": {
          "name": "Secondary"
        }
      },
      "params": {
        "R": {
          "name": "Ratio",
          "value": 1
        }
      },
      "constitutive_relations": [
        "e_1 - R*f_0",
        "e_1 + R*f_1"
      ]
    }
  }
}
