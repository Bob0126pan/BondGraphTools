{
  "desc": "Basic Components",
  "id":"base",
  "components": {
    "0": {
      "desc": "Equipotential Node",
      "cls": "ManyPort",
      "string": "0",
      "ports": null,
      "constitutive_relations": [
        "sum(f_i)",
        "e_i - e_0"
      ]
    },
    "1": {
      "desc": "Equal Flow Node",
      "cls": "ManyPort",
      "string": "1",
      "ports": null,
      "constitutive_relations": [
        "sum(e_i)",
        "f_i - f_0"
      ]
    },
    "R": {
      "desc": "Generalised Linear Resistor",
      "cls": "OnePort",
      "ports": {
        "0": null
      },
      "local_params": {
        "r": {
          "desc": "Resistance",
          "distribution": "Normal",
          "default": 1000,
          "std_dev": 10,
          "bounds": [
            0,
            "inf"
          ]
        }
      },
      "constitutive_relations": [
        "e_0 - f_0*r"
      ]
    },
    "C": {
      "desc": "Generalised Linear Capacitor",
      "cls": "OnePort",
      "ports": {
        "0": null
      },
      "local_params": {
        "c": {
          "desc": "Capacitance",
          "distribution": "Normal",
          "default": 1000,
          "std_dev": 10,
          "bounds": [
            0,
            "inf"
          ]
        }
      },
      "constitutive_relations": [
        "q_0 - c*e_0"
      ]
    },
    "I": {
      "desc": "Generalised Linear Inductor",
      "cls": "OnePort",
      "ports": {
        "0": null
      },
      "local_params": {
        "L": {
          "desc": "Inductance",
          "default": 2000,
          "bounds": [
            0,
            "inf"
          ]
        }
      },
      "constitutive_relations": [
        "p_0 -  - L*f_0"
      ]
    },
    "Se": {
      "desc": "Effort Source",
      "cls": "OnePort",
      "ports": {
        "0": null
      },
      "local_params": {
        "e": null
      },
      "constitutive_relations": [
        "e_0 - e"
      ]
    },
    "Sf": {
      "desc": "Flow Source",
      "cls": "OnePort",
      "ports": {
        "0": null
      },
      "local_params": {
        "f": null
      },
      "constitutive_relations": [
        "f_0 - f"
      ]
    },
    "P": {
      "desc": "Port",
      "cls": "IOPort",
      "sockets":{
        "0": ["u_e, u_f"]
      },
      "ports": {
        "0": null
      },
      "constitutive_relations": [
        "f_0 - u_f",
        "e_0 - u_e"
      ]
    },
    "TF": {
      "desc": "Linear Transformer",
      "cls": "TwoPort",
      "ports": {
        "0": {
          "Desc": "Primary"
        },
        "1": {
          "Desc": "Secondary"
        }
      },
      "local_params": {
        "a": {
          "desc": "Ratio",
          "defaults": 1,
          "bounds": [
            0,
            "inf"
          ]
        }
      },
      "constitutive_relations": [
        "e_1 - a*e_0",
        "f_0 - a*f_1"
      ]
    },
    "GY": {
      "desc": "Linear Gyrator",
      "cls": "TwoPort",
      "ports": {
        "0": {
          "Desc": "Primary"
        },
        "1": {
          "Desc": "Secondary"
        }
      },
      "local_params": {
        "R": {
          "desc": "Ratio",
          "defaults": 1,
          "bounds": [
            0,
            "inf"
          ]
        }
      },
      "constitutive_relations": [
        "e_1 - R*f_0",
        "e_1 + R*f_1"
      ]
    }
  }
}
