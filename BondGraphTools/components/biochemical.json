{
  "name": "Biochemical Components",
  "id":"BioChem",
  "components":{
    "Ce":{
      "name":"Concentration of Chemical Species",
      "class": "BaseComponent",
      "ports":{
        "0":null
      },
      "params":{
        "e0":{"name":"Standard Free Energy",
              "bounds":[0, "inf"]},
        "R":{"name":"Universal Gas Constant"},
        "T":{"name": "Temperature"},
        "V":{"name": "Volume of the vessel"}
      },
      "state_vars":{
        "q_0":"Molar Quantity"
      },
      "constitutive_relations":[
        "e_0 - e0 - R*T*log(q_0/V)"
      ]
    },
    "Re":{
      "name": "Biochemical Reaction",
      "class": "BaseComponent",
      "ports":{
        "0": {"name": "Primary"},
        "1": {"name": "Secondary"}
      },
      "params":{
        "k":{"name":"Reaction Rate"},
        "R":{"name":"Universal Gas Constant"},
        "T":{"name": "Temperature"}
      },
      "constitutive_relations":[
        "f_0 + f_1",
        "f_0 - k *(exp(e_0/R/T) - exp(e_1/R/T))"
      ]
    },
  "Y":{
    "name": "Stoichiometry",
    "class": "NPortWeighted",
    "ports":{
      "0": "Complex",
      "i": null
    },
    "constitutive_relations":[
      "e_0 - sum(c_i*e_i)",
      "f_0 + f_i/c_i"
    ]
  }
  }
}