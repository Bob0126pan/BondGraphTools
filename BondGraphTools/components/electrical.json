{
  "name": "Electrical Components",
  "id":"elec",
  "components": {
    "Di": {
      "name":"Diode",
      "class": "BaseComponent",
      "ports":{
        "0":null
      },
      "params":{
        "Is":{"name":"Forward Current"},
        "n": {"name":"Material Number"},
        "Vb":{"name":"Breakdown Voltage"}
      },
     "constitutive_relations":[
       "f_0 - Is*(exp(e_0/n/Vb) - 1)"
     ]
    },
    "BJT":{
      "name": "Bipolar Junction Transistor",
      "class": "BaseComponent",
      "ports":{
        "0": {"name": "Collector"},
        "1": {"name": "Base"},
        "2": {"name": "Emitter"}
      },
      "params": {
        "Is":{
          "name":"Reverse Saturation Current",
          "value": "10**-12",
        },
        "beta_f":{
          "name":"Forward common current emitter gain"
          "value": 100,
          "range":[20,500]
        },
        "beta_r":{
          "name":"Reverse common current emitter gain",
          "value":5,
          "range":[0,20]
        },
        "Vt":{
          "name":"Thermal Voltage kT/q"
          "value": 0.026
        }
      },
      "constitutive_relations":[
        "f_0 + f_1+ f_2",
        "f_1 - Is*((exp((e_1 - e_2)/Vt) - 1)/beta_f + (exp((e_1 -e_0)/Vt) -1)/beta_r)",
        "f_2 - Is*((exp((e_1 - e_2)/Vt) - exp((e_1 -e_0)/Vt)) + (exp((e_1 -e_2)/Vt) -1)/beta_f)"
      ]
    }
  }
}